<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import io.github.palexdev.materialfx.controls.MFXTextField?>

<VBox fx:id="root" xmlns:fx="http://javafx.com/fxml" fx:controller="parse.GUIController" alignment="CENTER">
	<MFXTextField fx:id="userInput" floatMode="BORDER" promptText="Enter text here" />
	<MFXButton fx:id="btn" text="Click here" onAction="#handleButtonAction" />
	<Label fx:id="messageLabel" />
</VBox>
